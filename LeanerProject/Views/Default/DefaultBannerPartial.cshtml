@model List<LearnerProject.Models.Entities.Banner>
<div class="untree_co-hero overlay" style="background-image: url('/Templates/learner-1.0.0/images/hero-img-1-min.jpg'); ">


    <div class="container">
        <div class="row align-items-center justify-content-center">

            <div class="col-12">

                <div class="row justify-content-center ">

                    <div class="col-lg-6 text-center ">
                        @foreach (var item in Model)
                        {

                            <h2 class="mb-4  font-weight-bolder text-white" data-aos="fade-up" data-aos-delay="100">@item.Title</h2>
                        }


                    </div>


                </div>

            </div>

        </div>
    </div>

</div>
