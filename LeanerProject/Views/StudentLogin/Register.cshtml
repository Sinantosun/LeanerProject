
@{
    Layout = null;
}


@{
    Layout = null;
}


<!doctype html>
<html lang="tr">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="author" content="Untree.co">
    <link rel="shortcut icon" href="favicon.png">

    <meta name="description" content="" />
    <meta name="keywords" content="bootstrap, bootstrap4" />

    <link href="https://fonts.googleapis.com/css2?family=Display+Playfair:wght@400;700&family=Inter:wght@400;700&display=swap" rel="stylesheet">


    <link rel="stylesheet" href="https://unicons.iconscout.com/release/v4.0.0/css/line.css">

    <link rel="stylesheet" href="~/Templates/learner-1.0.0/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Templates/learner-1.0.0/css/animate.min.css">
    <link rel="stylesheet" href="~/Templates/learner-1.0.0/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Templates/learner-1.0.0/css/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/Templates/learner-1.0.0/css/jquery.fancybox.min.css">
    <link rel="stylesheet" href="~/Templates/learner-1.0.0/fonts/icomoon/style.css">
    <link rel="stylesheet" href="~/Templates/learner-1.0.0/fonts/flaticon/font/flaticon.css">
    <link rel="stylesheet" href="~/Templates/learner-1.0.0/css/aos.css">
    <link rel="stylesheet" href="~/Templates/learner-1.0.0/css/style.css">

    <title>Learner Free Bootstrap Template by Untree.co</title>
</head>

<body>



    <div class="untree_co-hero inner-page overlay" style="background-image: url('/Templates/learner-1.0.0/images/img-school-5-min.jpg');">
        <div class="container">
            <div class="row align-items-center justify-content-center">
                <div class="col-12">
                    <div class="row justify-content-center ">
                        <div class="col-lg-6 text-center ">
                            <h1 class="mb-4 heading text-white" data-aos="fade-up" data-aos-delay="100">Yeni Öğrenci Kaydı</h1>

                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>

    <div class="untree_co-section">
        <div class="container">

            <div class="row mb-5 justify-content-center">
                <div class="col-lg-5 " data-aos="fade-up" data-aos-delay="200">
                    <form method="post" class="form-box">
                        <div class="row">
                            <small>Aşağıdaki Bilgileri Eksiksiz Doldurunuz</small>
                            <div class="col-12 mb-3 mt-3">
                                <input type="text" name="NameSurname" class="form-control" placeholder="Adınız Soyad">
                            </div>
                            <div class="col-12 mb-3 mt">

                                <input type="text" name="UserName" class="form-control" placeholder="Kullanıcı Adı">
                            </div>
                            <div class="col-12 mb-3">
                                <input type="password" name="Password" class="form-control" placeholder="Şifre">
                            </div>

                            <div class="col-12">
                                <input type="submit" value="Kaydol" class="btn btn-primary">
                                <p style="color:red;">@Html.ValidationSummary()</p>
                            </div>
                        </div>
                    </form>
                    <br />
                    @if (TempData["Result"] != null)
                    {
                        <div class="alert alert-primary">
                            @Html.Raw(TempData["Result"])
                        </div>
                    }
                </div>
            </div>


        </div>
    </div> <!-- /.untree_co-section -->

  


    <div id="overlayer"></div>
    <div class="loader">
        <div class="spinner-border" role="status">
            <span class="sr-only">Loading...</span>
        </div>
    </div>

    <script src="~/Templates/learner-1.0.0/js/jquery-3.4.1.min.js"></script>
    <script src="~/Templates/learner-1.0.0/js/popper.min.js"></script>
    <script src="~/Templates/learner-1.0.0/js/bootstrap.min.js"></script>
    <script src="~/Templates/learner-1.0.0/js/owl.carousel.min.js"></script>
    <script src="~/Templates/learner-1.0.0/js/jquery.animateNumber.min.js"></script>
    <script src="~/Templates/learner-1.0.0/js/jquery.waypoints.min.js"></script>
    <script src="~/Templates/learner-1.0.0/js/jquery.fancybox.min.js"></script>
    <script src="~/Templates/learner-1.0.0/js/jquery.sticky.js"></script>
    <script src="~/Templates/learner-1.0.0/js/aos.js"></script>
    <script src="~/Templates/learner-1.0.0/js/custom.js"></script>

</body>

</html>
